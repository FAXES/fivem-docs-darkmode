{
   "name": "FiveM/RedM Dark Mode",
   "description": "This extension changes the theme on the FiveM Documentation websites to be dark mode where the operating system has it enabled.",
   "version": "1.2",
   "manifest_version": 3,
   "content_scripts": [{
      "matches": ["https://docs.fivem.net/*", "https://redm.disquse.me/*"],
      "css": ["docs.css"]
   }],

   "action": {
      "default_title": "FiveM/RedM Dark Mode",
      "default_icon": {
         "16": "icons/icon16.png",
         "32": "icons/icon32.png",
         "48": "icons/icon48.png",
         "128": "icons/icon128.png"
      }
   }
}
